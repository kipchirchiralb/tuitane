<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Login</title>
    <link rel="stylesheet" href="styles/main.css" />
  </head>
  <body>
    <%- include("_header.ejs") %>
    <main>
      <h1>Login here</h1>
      <form action="">
        <input
          type="email"
          name="email"
          id="email"
          placeholder="example@email.com"
        />
        <br />
        <input
          type="password"
          name="password"
          id="pass"
          placeholder="Password"
        />
        <br />
        <input type="submit" value="Login" />
      </form>
      <a href="#">Forgot Pasword?</a>
    </main>
    <%- include("_footer.ejs") %>
  </body>
</html>
